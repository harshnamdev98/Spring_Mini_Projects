<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

   <!-- Spring Bean cfg -->
    <bean id="computerSystem" name="pc,machine" class="com.nt.beans.ComputerSystem">
       <property name="sysId"  value="1001"/>
       <property name="labName"  value="NIT Lab"/>
    </bean>

  <alias name="computerSystem" alias="sys"/>
  <alias name="computerSystem" alias="system"/>

</beans>
